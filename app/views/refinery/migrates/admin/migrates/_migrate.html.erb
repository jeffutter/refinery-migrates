<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(migrate) -%>">
  <span class='title'>
    <%= migrate.title %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.migrates_migrate_path(migrate),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_migrates_admin_migrate_path(migrate),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.migrates_admin_migrate_path(migrate),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => migrate.title),
        :method => :delete %>
  </span>
</li>
