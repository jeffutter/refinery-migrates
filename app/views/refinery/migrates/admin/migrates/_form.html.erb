<%= form_for [refinery, :migrates_admin, @migrate] do |f| -%>
  <%= render '/refinery/admin/error_messages',
              :object => @migrate,
              :include_object_name => true %>

  
  <div class='field'>
    <%= f.label :title -%>
    <%= f.text_field :title, :class => 'larger widest'  -%>

  </div>
  
  <%= render '/refinery/admin/form_actions', :f => f,
             :continue_editing => false,
             :delete_title => t('delete', :scope => 'refinery.migrates.admin.migrates.migrate'),
             :delete_confirmation => t('message', :scope => 'refinery.admin.delete', :title => @migrate.title) %>
<% end -%>
